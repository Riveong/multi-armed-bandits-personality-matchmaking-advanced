   "cell_type": "code",
   "execution_count": 21,
   "id": "4265afc6",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2024-05-16T02:07:09.393601Z",
     "iopub.status.busy": "2024-05-16T02:07:09.393177Z",
     "iopub.status.idle": "2024-05-16T02:07:09.402900Z",
     "shell.execute_reply": "2024-05-16T02:07:09.401713Z"
    },
    "papermill": {
     "duration": 0.032445,
     "end_time": "2024-05-16T02:07:09.405457",
     "exception": false,
     "start_time": "2024-05-16T02:07:09.373012",
     "status": "completed"
