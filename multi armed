    "def plot_results_specific(results, keys_to_plot):\n",
    "    plt.figure(figsize=(10, 6))  # Define figure size\n",
    "    for key in keys_to_plot:\n",
    "        if key in results:\n",
    "            folds, rewards = zip(*results[key])  # Ensure you're unpacking correctly\n",
    "            plt.plot(folds, rewards, marker='o', label=key)\n",
    "        else:\n",
    "            print(f\"Warning: Key '{key}' not found in results.\")\n",
    "    plt.legend()\n",
    "    plt.xlabel('Fold Index')\n",
    "    plt.ylabel('Total Rewards')\n",
    "    plt.title('Comparison of Specific Strategies')\n",
    "    plt.grid(True)\n",
    "    plt.show()"
   ]
  },
